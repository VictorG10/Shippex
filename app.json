{
  "expo": {
    "name": "Shippex",
    "slug": "Shippex",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/shippex-logo2-square.png",
    "scheme": "shippex",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.galaxies.firebase"
      // "googleServicesFile": "./GoogleService-Info.plist"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/shippex-logo2-square.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.galaxies.firebase",
      // "googleServicesFile": "./google-services.json",
      "edgeToEdgeEnabled": true
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/shippex-logo2-square.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/shippex-logo2-square.png",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#ffffff"
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true
    }
  }
}
